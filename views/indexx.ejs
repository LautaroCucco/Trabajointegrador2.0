<!DOCTYPE html>
<html lang="es">

<!--Include Head-->

<%- include('./partials/head') %>

<body>
<% if (locals.user == undefined) { %>
	<%- include('./partials/headerNoLogueado') %> 
<% } else { %>
 <%- include('./partials/headerLogueado')  %>
<% } %>

	
	<!-- Banner -->
<!-- 	<div class="banner">
		<img src="./images/bannerprog2.jpg" alt="Banner de ofertas">
	</div> -->
	<!-- /Banner -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Novedades</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for (let i = 0; i < 4; i++) { %>
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="/productos/id/<%= producto[i].id %>">
						<figure class="product-box_image">
							<img src="<%= producto[i].photo %>"  alt="Autos">
						</figure>
						<article class="product-box_data">
							<h2> <%= producto[i].model  %>  </h2>
							<p> <%= producto[i].description  %> </p>
							<p><%=// producto[i].comentarios %></p>
						</article>
					</a>
				</section>
			</div>
			<% } %>
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">MÃ¡s comentados</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for (let i = 4; i < 8; i++) { %>
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/productos/id/<%= producto[i].id %>">
							<figure class="product-box_image">
								<img src="<%= producto[i].photo %>"  alt="Autos">
							</figure>
							<article class="product-box_data">
								<h2> <%= producto[i].model  %>  </h2>
								<p> <%= producto[i].description %> </p>
								<p><%= //producto[i].comentarios %></p>
							</article>
						</a>
					</section>
				</div>
				<% } %>
			</div>
		</div>
			
		</div>
	</div>
	<!-- footer -->
	<%- include('./partials/footer')  %> 
	<!-- /footer -->
</body>
</html>
			